{
  "compilerOptions": {
    "target": "es5",
    "declaration": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "outDir": "lib",
    "preserveConstEnums": true,
    "removeComments": true,
    "strictNullChecks": true,
    "lib": [
      "es2015.promise",
      "es5"
    ]
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
